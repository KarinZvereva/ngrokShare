            <Button layoutX="15.0" layoutY="440.0" mnemonicParsing="false" onAction="#addProfile" prefHeight="25.0" prefWidth="141.0" text="Add" />
            <Button layoutX="169.0" layoutY="440.0" mnemonicParsing="false" onAction="#deleteProfile" prefHeight="25.0" prefWidth="141.0" text="Delete" />
            <TextField fx:id="textValue" layoutX="61.0" layoutY="407.0" prefHeight="25.0" prefWidth="92.0" />
            <Label layoutX="14.0" layoutY="411.0" text="Value:" />
